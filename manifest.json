{
  "manifest_version": 2,
  "name": "Skribbl Typo",
  "description": "The most complete toolbox for skribbl.io with namefinder & tablet support",
  "version": "21.5.2",
  "author": "tobeh",
  "icons": {
    "128": "res/icon128.png"
  },
  "browser_action": {
    "default_title": "Skribbl Typo",
    "default_icon": "res/icon128.png",
    "title": "Dashboard",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "gamePatch.js",
    "res/*",
    "gifCap/*",
    "features/*"
  ],
  "permissions": [
    "activeTab",
    "<all_urls>",
    "webRequest"
  ],
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "*://skribbl.io/",
        "*://skribbl.io/*"
      ],
      "js": [
        "jquery/jquery-3.6.0.min.js",
        "jquery/tooltips/jquery-ui.min.js",
        "bootstrap/popper.min.js",
        "bootstrap/js/bootstrap.min.js",
        "picker/colr_pickr.min.js",
        "color.js",
        "features/commands.js",
        "features/capture.js",
        "features/typro.js",
        "features/drops.js",
        "features/imageTools.js",
        "features/emojis.js",
        "features/uiTweaks.js",
        "features/agent.js",
        "features/pressure.js",
        "features/imageOptions.js",
        "features/accessibility.js",
        "features/gamemode.js",
        "features/keybinds.js",
        "content.js"
      ],
      "css": [
        "bootstrap/css/bootstrap.min.css",
        "features/style.css",
        "picker/colr_pickr.min.css"
      ]
    },
    {
      "run_at": "document_start",
      "matches": [
        "*://skribbl.io/",
        "*://skribbl.io/*"
      ],
      "js": [
        "features/modal.js",
        "features/sprites.js",
        "features/genericFunctions.js",
        "features/visuals.js",
        "errors.js",
        "features/socket.js",
        "features/lobbies_.js",
        "patcher.js"
      ]
    },
    {
      "matches": [
        "*://sketchful.io/",
        "*://sketchful.io/*"
      ],
      "js": [
        "features/sketchfulReport.js"
      ]
    }
  ]
}